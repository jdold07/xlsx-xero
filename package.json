    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "-": "^0.0.1",
